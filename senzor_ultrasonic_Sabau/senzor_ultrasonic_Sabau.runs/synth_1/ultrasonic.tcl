}
foreach dcp [get_files -quiet -all *.dcp] {
  set_property used_in_implementation false $dcp
}
read_xdc E:/FPGA/senzor_ultrasonic_Sabau/senzor_ultrasonic_Sabau.srcs/constrs_1/new/constr.xdc
