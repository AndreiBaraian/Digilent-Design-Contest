set_property default_lib xil_defaultlib [current_project]
set_property target_language VHDL [current_project]
set_property board_part digilentinc.com:zybo:part0:1.0 [current_project]
set_property ip_output_repo e:/FPGA/senzor_ultrasonic_Sabau/senzor_ultrasonic_Sabau.cache/ip [current_project]
set_property ip_cache_permissions {read write} [current_project]
read_vhdl -library xil_defaultlib {
  E:/FPGA/senzor_ultrasonic_Sabau/senzor_ultrasonic_Sabau.srcs/sources_1/new/triggerGenerator.vhd
  E:/FPGA/senzor_ultrasonic_Sabau/senzor_ultrasonic_Sabau.srcs/sources_1/new/echo_period.vhd
